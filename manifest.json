{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "Form Submit Helper",
    "short_name": "amigos extension",
    "description": "On form submit values capture",
    "icons": {
      "16": "icons/16x16.png",
      "32": "icons/32x32.png",
      "48": "icons/48x48.png",
      "64": "icons/64x64.png",
      "128": "icons/128x128.png"
    },
    "content_scripts": [
      {
        "run_at": "document_end",
        "matches": ["https://amigos.ayu.health/*"],
        "js": ["content.js"]
      }
    ]
   }

   
   